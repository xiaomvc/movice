<import src="../movice-star/movice-star-template.wxml" />
<template name="movice-list">
        <view class="movice-list" data-id="{{item.id}}" catchtap='getDetail'>
            <image class="listImg" src="{{item.image}}"></image>
            <text>{{item.title}}</text>
            <template is='movice-star' data="{{...item}}"/>
        </view>
</template>